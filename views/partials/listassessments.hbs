
<table class="ui table">
    <thead>
    <tr>
        <th> Date </th>
        <th> Weight </th>
        <th> Chest </th>
        <th> Thigh </th>
        <th> Upper Arm </th>
        <th> Waist </th>
        <th> Hips </th>
        <th> Comment </th>
    </tr>
    </thead>
    <tbody>
{{#each userAssessment}}
        <tr>

            <td>
                {{date}}
            </td>
            <td>
                {{weight}}kgs
            </td>
            <td>
                {{chest}}cm
            </td>
            <td>
                {{thigh}}cm
            </td>
            <td>
                {{upperArm}}cm
            </td>
            <td>
                {{waist}}cm
            </td>
            <td>
                {{hips}}cm
            </td>

            <td>
                {{comment}}
            </td>

        </tr>
{{/each}}
    </tbody>
</table>
